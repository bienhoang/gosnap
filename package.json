{
  "name": "@optimo/gosnap",
  "author": "Bien Hoang <me@bienhoang.xyz>",
  "version": "1.0.0",
  "description": "Annotate any website, generate structured visual feedback for AI agents â€” works as React component or standalone embed",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "gosnap": "./dist/cli.js"
  },
  "unpkg": "./dist/embed.global.js",
  "jsdelivr": "./dist/embed.global.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./embed": {
      "types": null,
      "default": "./dist/embed.global.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && chmod +x dist/cli.js",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "lucide-react": ">=0.263.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "optionalDependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.7.0"
  },
  "keywords": [
    "gosnap",
    "ai-feedback",
    "ai-agent",
    "visual-feedback",
    "ui-annotation",
    "structured-feedback",
    "ai-coding",
    "ui-review",
    "element-inspector",
    "feedback",
    "annotation",
    "react",
    "nextjs",
    "web-component",
    "embeddable",
    "vanilla-js",
    "vibe-kanban",
    "webhook"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bienhoang/gosnap.git"
  },
  "homepage": "https://github.com/bienhoang/gosnap#readme",
  "bugs": {
    "url": "https://github.com/bienhoang/gosnap/issues"
  },
  "sideEffects": false
}
