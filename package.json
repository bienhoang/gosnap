{
  "name": "pro-ui-feedbacks",
  "author": "Bien Hoang <me@bienhoang.xyz>",
  "version": "0.2.1",
  "description": "Visual UI Feedback for React/Next.js apps",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "lucide-react": ">=0.263.0"
  },
  "devDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "typescript": "^5.7.0",
    "tsup": "^8.4.0"
  },
  "keywords": [
    "react",
    "nextjs",
    "feedback",
    "ui-feedback",
    "annotation",
    "inspector",
    "floating-toolbar",
    "design-review",
    "qa",
    "toolbar",
    "widget"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bienhoang/pro-ui-feedbacks.git"
  },
  "homepage": "https://github.com/bienhoang/pro-ui-feedbacks#readme",
  "bugs": {
    "url": "https://github.com/bienhoang/pro-ui-feedbacks/issues"
  },
  "sideEffects": false
}